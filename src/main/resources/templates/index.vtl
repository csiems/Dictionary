<h1>Address Book</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      #foreach ($contact in $contacts)
      <td><a href="/index/$contact.getId()">$contact.getFirstName() $contact.getLastName()</a></td>
    </tr>
      #end
  </tbody>
</table>
<br>

<a href="/index/new" class="btn btn-info form-control">Add A New Contact</a>
